{
    "name": "dubbing-worker",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "dev": "wrangler dev",
        "deploy": "wrangler deploy",
        "db:create": "wrangler d1 create dubbing-db",
        "db:migrate": "wrangler d1 execute dubbing-db --file=./schema.sql",
        "db:migrate:local": "wrangler d1 execute dubbing-db --local --file=./schema.sql"
    },
    "dependencies": {
        "hono": "^4.0.0"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20240117.0",
        "typescript": "^5.3.3",
        "wrangler": "^3.24.0"
    }
}